generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// --- user 
model User {
  id           Int     @id @default(autoincrement())
  userName     String
  email        String
  password     String
  profileImage String?
  role         String  @default("user")
}
